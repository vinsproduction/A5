<? if (isset($status["errmsg"])): ?>
	alert('<?= j($status["errmsg"]) ?>');
<? else: ?>
	var nodes = [];
	var load_node_childs = [];
	<? foreach ($status["affected_nodes"] as $item): ?>
		nodes.push('<?= h($item["id"]) ?>');
		load_node_childs.push('<?= h($item["parent_id"]) ?>');
	<? endforeach ?>
	expand_tree(nodes, { 'load_node_childs': load_node_childs });
<? endif ?>